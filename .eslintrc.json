{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "object-curly-spacing": ["error", "always"],
        "arrow-parens": ["error", "as-needed"],
        "comma-dangle": ["error", "only-multiline"]
    }
}
